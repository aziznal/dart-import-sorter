{
  "name": "dart-import-sorter",
  "repository": {"url": "https://github.com/aziznal/dart-import-sorter"},
  "displayName": "Dart Import Sorter",
  "description": "Sorts and groups import statements in your dart files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onCommand:dartimportsorter.sortImports"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dartimportsorter.sortImports",
        "title": "Dart: Sort Imports"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.65.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "typescript": "^4.5.5"
  }
}
